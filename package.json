{
  "name": "node_api",
  "version": "1.0.0",
  "description": "NodeJS API",
  "main": "dist/server.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "build": "npm run tslint && npm run copy-static-assets && npm run build-ts",
    "build-ts": "node_modules/typescript/bin/tsc",
    "watch": "concurrently -k -p \"[{name}]\" -n \"TypeScript,Node\" -c \"cyan.bold,green.bold\" \"npm run watch-ts\" \"npm run serve\"",
    "watch-ts": "node_modules/typescript/bin/tsc -w",
    "tslint": "node_modules/tslint/bin/tslint -c tslint.json -p tsconfig.json",
    "serve": "node dist/server.js",
    "test": "node_modules/.bin/mocha --timeout 4000 --require ts-node/register src/test/**/*.spec.ts --exit",
    "test1": "node_modules/.bin/mocha --timeout 4000 --require ts-node/register src/test/models/containerModel.spec.ts --exit",
    "copy-static-assets": "node copyStaticAssets.js",
    "docs": "apidoc -i src/routes/ -o src/public/docs",
    "all": "npm run docs && npm run build && npm run test && npm run serve",
    "dev": "npm run build && npm run serve",
    "debug": "node --inspect dist/server.js"
  },
  "author": "Matt Gabriel",
  "license": "ISC",
  "dependencies": {
    "@types/lodash": "^4.14.116",
    "@types/passport": "^1.0.0",
    "@types/qs": "^6.5.1",
    "@types/redis": "^2.8.7",
    "@types/request": "^2.47.1",
    "@types/request-promise-native": "^1.0.15",
    "@types/xml2js": "^0.4.3",
    "aws-sdk": "^2.224.1",
    "bcryptjs": "^2.4.3",
    "bunyan": "^1.8.12",
    "cerialize": "^0.1.18",
    "chai": "^4.1.2",
    "compress-str": "^1.0.2",
    "crypto": "^1.0.1",
    "dotenv": "^4.0.0",
    "fs": "0.0.1-security",
    "jimp": "^0.2.28",
    "jwt-simple": "^0.5.1",
    "knex": "^0.14.2",
    "lodash": "^4.17.11",
    "mandrill-api": "^1.0.45",
    "md5": "^2.2.1",
    "mocha": "^4.0.1",
    "moment": "^2.22.1",
    "mz": "^2.7.0",
    "passport": "^0.4.0",
    "passport-jwt": "^2.2.1",
    "pg": "^7.4.0",
    "qs": "^6.5.2",
    "random-id": "0.0.2",
    "redis": "^2.8.0",
    "reflect-metadata": "^0.1.10",
    "request": "^2.88.0",
    "request-promise-native": "^1.0.5",
    "restify": "^6.3.2",
    "restify-cors-middleware": "^1.0.1",
    "restify-jwt": "^0.4.0",
    "restify-plugins": "^1.6.0",
    "serve-static-restify": "^2.0.0",
    "ts-node": "^3.3.0",
    "urlencode": "^1.1.0",
    "xml2js": "^0.4.19"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/chai": "^4.0.8",
    "@types/dotenv": "^4.0.2",
    "@types/jwt-simple": "^0.5.33",
    "@types/knex": "0.0.67",
    "@types/mandrill-api": "^1.0.30",
    "@types/md5": "^2.1.32",
    "@types/mocha": "^2.2.44",
    "@types/passport-jwt": "^3.0.1",
    "@types/pg": "^7.4.0",
    "@types/reflect-metadata": "0.0.5",
    "@types/restify": "^5.0.6",
    "@types/restify-cors-middleware": "^1.0.1",
    "@types/restify-errors": "^4.3.2",
    "@types/restify-plugins": "^1.5.11",
    "@types/typescript": "^2.0.0",
    "shelljs": "^0.7.8",
    "tslint": "^5.8.0",
    "typescript": "^2.6.1"
  }
}
